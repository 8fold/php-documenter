{
    "name": "8fold/documenter-php",
    "description": "A standalone library that wraps phpDocumentor for use with frameworks supporting PHP packages.",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@joshuabruce.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "phpdocumentor/reflection": "3.0.*",
        "8fold/html5-generator-php": "0.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "phpDocumentor\\Reflection\\": "vendor/phpdocumentor/reflection/src/phpDocumentor/Reflection",
            "Eightfold\\DocumenterPhp\\": "src/",
            "Eightfold\\Html5Gen\\": "vendor/8fold/html5-generator-php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eightfold\\DocumenterPhp\\Tests\\": "tests/"
        }
    }
}
